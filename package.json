{
  "name": "oj-frontend",
  "version": "1.0.0",
  "description": "frontend part of oj",
  "author": "creedowl",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
